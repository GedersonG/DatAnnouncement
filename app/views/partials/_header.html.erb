<%= link_to products_path do %>
    <h1> DatAnnouncement </h1>
<% end %>
<% if !Current.user %>
  <%= link_to t('.register'), new_user_path %>
  <%= link_to t('.login'), new_session_path%>
<% else %>
  <%= link_to t('.add'), new_product_path %>
  <%= link_to t('.categories'), categories_path %>
  <%= link_to t('.logout'), session_path(Current.user.id), data: {
    turbo_method: :delete,
    turbo_confirm: t('common.confirm')
  } %>
<% end %>